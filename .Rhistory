n=100
x=NULL
for (i in 1:n) {
u=runif(1)
p=0.4
if(u<p) x[i]=1 else x[i]=0
}
x
mean(x)
mle.bernouli=function()
{
-(sum(x)*log(p) + (n-sum(x))* log(1-p))
}
optim(0.4,mle.bernouli)
