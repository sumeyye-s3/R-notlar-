n=100
mu=2
sigma=4.5
x=rnorm(n,mu,sigma)
log.olab=function(par)
{
-(1/sqrt(2*pi))^n*1/par[2]^n*exp(-1/2*sum((x-par[1])/par[2])^2)
return(log.olab) # - ile carpariz maxim yapmak icinn
}
optim(c(2,3),log.olab,method = "BFGS")
